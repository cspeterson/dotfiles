alias apt-provides="echo 'NOTE: You may want to \`sudo apt-file update...\`'; apt-file find"
alias aptinst='apt --yes install'
alias aptls='apt list --installed'
alias aptrm='apt remove'
alias apts='apt-cache search'
alias aptsh='apt-cache show'
alias aptshow='apt-cache show'
alias aptupd='apt update'
alias aptupg='apt --yes upgrade'
